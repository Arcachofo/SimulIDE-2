        <register name="OSCTUNE" addr="0x90" bits=""/>
        <register name="ANSEL"  addr="0x91" bits=""/>
        <register name="PR2"    addr="0x92" bits="" reset="11111111"/>
        
        <register name="WPUA"   addr="0x95" reset="00000010" mask="11110101"
